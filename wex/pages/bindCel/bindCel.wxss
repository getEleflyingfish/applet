.cel-list-wap {
    width: 750rpx;
    padding: 0 28rpx;
    box-sizing: border-box;
}

.cel-list {
    border: 1rpx solid #E6E6E6;
    width: 100%;
    border-radius: 10rpx;
    margin-bottom: 24rpx;
    padding-top: 12rpx;
    padding-bottom: 12rpx;
    position: relative;
}


.cel-botton {
    background: #99cc33 !important;
    color: #ffffff !important;
    border-radius: 10rpx !important;
    margin: 0 !important;
}

.cel-list {
    display: flex;
    align-items: center;
    text-align: left;
}

.cel-list .macom {
    width: 70rpx;
    margin-left: 15rpx;
    position: relative;

}

.cel-list .section {
    font-size: 30rpx;
    padding-left: 30rpx;
  /*   width: 80%; */
}

.cel-list .section input {
    height: 70rpx;
    line-height: 70rpx;
    display: block;
}

.cel-list .macom::after {
    content: "";
    border-right: 1rpx solid #cdcdcd;
    margin-top: 1rpx;
    display: block;
    width: 0rpx;
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;

}

.cel-list-wap .gain {
    margin: 0 !important;
    position: absolute;
    right: 30rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 10rpx;
    color: #ffffff;
    background: #99cc33;
    width: 186rpx;
    font-size: 26rpx;

}

.cel-list-wap .code {}

.mt30 {
    margin-top: 30rpx;
}

.hint-i { 
    color: red;
    font-size: 26rpx;
    margin-bottom: 10rpx;
}