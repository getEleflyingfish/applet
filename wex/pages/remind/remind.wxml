            </view>
          </view>
        </view>


      </scroll-view>
    </view>


  </view>

</view>





<view class="" hover-class="none" hover-stop-propagation="false">
  <navigator class="" target="" url="/pages/person/person" hover-class="navigator-hover" open-type="navigate">
