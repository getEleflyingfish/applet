              <view class="hid-item fl" wx:for='{{linem}}' wx:for-index='key'> {{ linem[key]}}</view>
            </view>
          </view>
        </view>
        <!-- remind-item  ==列表 -->
        <view data-num="2" class="remind-item {{_num==2?'on':''}}" bindtap="menuClick">
